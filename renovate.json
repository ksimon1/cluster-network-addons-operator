{
    "extends": [
      ":gitSignOff",
      ":dependencyDashboard"
    ],
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "baseBranches": [
      "main",
      "release-0.91"
    ],
    "prConcurrentLimit": 3,
    "groupName": "all dependencies",
    "groupSlug": "all",
    "lockFileMaintenance": {
      "enabled": false
    },
    "labels": [
      "release-note-none"
    ],
    "ignorePaths": [
      "automation/**",
      "cluster/**",
      "hack/**",
      "test/**",
      "tools/**"
    ],
    "packageRules": [
      {
        "groupName": "all dependencies bump on main",
        "groupSlug": "all",
        "enabled": false,
        "matchBaseBranches": [
          "main"
        ],
        "matchPackagePatterns": [
          "*"
        ]
      },
      {
        "matchPackageNames": [
          "golang",
          "go"
        ],
        "allowedVersions": "<=1.20",
        "matchBaseBranches": [
          "release-0.91"
        ]
      }
    ],
    "vulnerabilityAlerts": {
      "enabled": true
    },
    "osvVulnerabilityAlerts": true,
    "assigneesFromCodeOwners": true,
    "separateMajorMinor": false,
    "postUpdateOptions": ["gomodTidy"]
}
